.js"></script><script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script><script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script><script>var currentHeight = 0
$(document).ready(function() {
  setTimeout(hideLoader, 1000) // max 1 second loader

  $('iframe').on('load', function() {
    hideLoader()
  });
  adjustIframe()
  $(window).resize(adjustIframe)

  $('.device-icon').click(function() {
    $('.device-icon').removeClass('active')
    $(this).addClass('active')
    $("#iframe").attr("width", $(this).data('width'))
    var height = $(this).data('height')
    if(height > (currentHeight + 30)) height = currentHeight - 30
    $("#iframe").attr("height", height)
    if($(this).data('device')) {
      $("#iframe").addClass('device-frame')
    } else {          
      $("#iframe").removeClass('device-frame')
    }
  })
})

function hideLoader() {
  document.getElementById('loaderWrapper').style.display = "none";
}

function adjustIframe() {
  currentHeight = ($(window).height() - $('#download-bar').height())
  iframe_height = currentHeight + "px"
  $("#iframe").attr("height", iframe_height)
}
</script><script src="/scripts/main.js?ver=1.5.0"></script></body></html>